#include <iostream>
using namespace std;
int main() {
int dollar ;
cout << "你有多少錢:";
cin >> dollar;
int A = dollar % 50;
int B = A % 10;
int C = B % 5;
int D = C % 1;

if (dollar >= 50)
cout << "有50元硬幣X" << dollar / 50 <<"個";
  
if (A >= 10)
cout << "\n有10元硬幣X" << A / 10 <<"個";

if (B >= 5)
cout << "\n有5元硬幣X" << B / 5 <<"個";

if (C >= 1)
cout << "\n有1元硬幣X" << C / 1 <<"個";

}
